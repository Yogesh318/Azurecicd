{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "inputfile",
						"type": "DatasetReference"
					},
					"name": "landing"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "staging",
						"type": "DatasetReference"
					},
					"name": "staging"
				}
			],
			"transformations": [],
			"scriptLines": [
				"parameters{",
				"     filename as string",
				"}",
				"source(allowSchemaDrift: false,",
				"     validateSchema: true,",
				"     limit: 10,",
				"     ignoreNoFilesFound: true,",
				"     wildcardPaths:[($filename)]) ~> landing",
				"landing sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:[($filename)],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> staging"
			]
		}
	}
}