{
	"name": "staging",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"customername": {
				"type": "String"
			},
			"jobname": {
				"type": "String"
			},
			"filename": {
				"type": "String"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().filename",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('stg_',replace(dataset().jobname,'.csv',''))",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().customername",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}